  "Data & Computation|Solution|SPM": {
    "title": "Platform-level Data & Computation Solution",
    "sections": [
      {
        "headline": "Protocol-level solutions for cloud data storage integrity [1]",
        "points": [
          {
            "label": "Remote data integrity verification protocol (Kumar & Subramanian, 2011)",
            "text": "Based on Elliptic Curve Cryptography and Sobol sequence; periodically verifies integrity of data stored at the CSP without retrieving the original data and generates probabilistic proofs of integrity.",
            "example": ""
          }
        ]
      }
    ],
    "tags": [
      "storage-integrity",
      "ECC",
      "probabilistic-proof"
    ],
    "refs": [
      {
        "title": "[1] An Efficient and Secure Protocol for Ensuring Data Storage Security in Cloud Computing",
        "link": "https://www.ijcsi.org/papers/IJCSI-8-6-1-261-274.pdf"
      }
    ]
  },
